import RaceTrack.*;
class Car
{
	private int year=2010;
	private String make="Porsche";
	private double speed=25.0;
	
	Car(int a, String b, double c)
	{
		this.year=a;
		this.String=b;
		this.speed=c;
	}


	public int getYear()
	{
		return this.year;
	}
	
	public String getMake()
	{
		return this.make;
	}
	
	public double getSpeed()
	{
		return this.speed;
	}
	
	void accelerate()
	{
		speed= speed + 1;
		System.out.println(getSpeed());
	}
}